@model CagroLab.Models.Lab

@{
    ViewData["Title"] = "Accounts for @Model.Lab_Name";
}

<h2>Accounts for @Model.Lab_Name</h2>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Accounts.First().Username)</th>
            <th>@Html.DisplayNameFor(model => model.Accounts.First().Full_Name)</th>
            <th>@Html.DisplayNameFor(model => model.Accounts.First().Last_Activity)</th>
            <th>@Html.DisplayNameFor(model => model.Accounts.First().Is_Active)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var account in Model.Accounts)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => account.Username)</td>
                <td>@Html.DisplayFor(modelItem => account.Full_Name)</td>
                <td>@Html.DisplayFor(modelItem => account.Last_Activity)</td>
                <td>@Html.DisplayFor(modelItem => account.Is_Active)</td>
            </tr>
        }
    </tbody>
</table>
