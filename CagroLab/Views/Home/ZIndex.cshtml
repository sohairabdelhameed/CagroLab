@model IEnumerable<CagroLab.Models.Sample>

<!DOCTYPE html>
<html>
<head>
    <title>Samples</title>
    <link rel="stylesheet" type="text/css" href="~/css/site.css" />
</head>
<body>
    <h1>Samples</h1>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Sample Type Id</th>
                <th>Patient Name</th>
                <th>Patient Id</th>
                <th>Patient Phone</th>
                <th>Account</th>
                <th>Package</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var sample in Model)
            {
                <tr>
                    <td>@sample.Id</td>
                    <td>@sample.Sample_Type_Id</td>
                    <td>@sample.Patient_Name</td>
                    <td>@sample.Patient_Id</td>
                    <td>@sample.Patient_Phone</td>
                    <td>@sample.Account_Id</td>
                    <td>@sample.Package_Id</td>
                    
                    
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
